{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/tasks",
      "destination": "/api/v1/tasks"
    }
  ],
  "static": {
    "directory": "/public"
  }
}
  